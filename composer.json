{
    "name": "hj/SimpleFileManager",
    "description": "An simple file manager using Silex Framework, Doctrine and Twig",
    "minimum-stability": "dev",
    "license": "proprietary",
    "authors": [
        {
            "name": "hatim jacquir",
            "email": "jacquirhatim@gmail.com"
        }
    ],
    "require": {
        "symfony/config": "~2.6",
        "phpunit/phpunit": "~4.0",
        "doctrine/orm": "~2.4",
        "mikey179/vfsStream": "~1.4",
        "twig/twig": "~1.0",
        "symfony/form": "~3.0",
        "symfony/twig-bridge": "~3.0",
        "symfony/translation": "~3.0",
        "symfony/security-csrf": "~3.0",
        "symfony/validator": "~3.0",
        "symfony/http-foundation": "~3.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/routing": "~3.0",
        "symfony/http-kernel": "~3.0"
    },
    "autoload": {
        "psr-0": {
            "Hj\\": "src/"
        }
    },
    "config": {
        "process-timeout": 600,
        "preferred-install": "dist",
        "github-protocols": ["https"]
    }
}
